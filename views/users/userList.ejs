<%- include('../partials/headers.ejs', {title: "List Users"}) %>

<body>
   <table>
      <tr>
        <th>Username</th>
        <th>Role</th>
        <th colspan="2">Action</th>
      </tr>
      
      <% usersData.forEach(user => { %> 
      <tr>
        <td> <%= user.username %> </td>
        <td> <%= user.role %> </td>
        <td><a href="/users/edit/<%= user.id %>">Edit</a> </td>
        <td><a href="/users/delete/<%= user.id %>">Delete</a> </td>
      </tr>
      <% }) %> 
    </table>
</body>

</html>