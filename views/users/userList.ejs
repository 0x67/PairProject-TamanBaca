<%- include('../partials/headers.ejs', {title: "List Users"}) %>



<body>
   <h5><a href="/users/add">Add New User</a></h5>
   <div class=" h-100 d-flex justify-content-center align-items" style="position: fixed;">
      <div>
         <table>
            <tr>
              <th>Username</th>
              <th>Role</th>
              <th colspan="2">Action</th>
            </tr>
            
            <% usersData.forEach(user => { %> 
            <tr>
              <td> <%= user.username %> </td>
              <td> <%= user.role %> </td>
              <td><a href="/users/edit/<%= user.id %>">Edit</a> </td>
              <td><a href="/users/delete/<%= user.id %>">Delete</a> </td>
            </tr>
            <% }) %> 
          </table>
      </div>
    </div>


   
</body>

</html>